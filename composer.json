{
    "name": "vdlp/oc-glide-plugin",
    "description": "Allows developers to easily manipulate images in October CMS powered websites.",
    "license": "GPL-2.0",
    "type": "october-plugin",
    "authors": [
        {
            "name": "Van der Let & Partners",
            "email": "octobercms@vdlp.nl"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "composer/installers": "^1.0 || ^2.0",
        "league/glide-laravel": "^1.0",
        "october/rain": "^2.0 || ^3.0"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.42"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://gateway.octobercms.com"
        }
    ],
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "ergebnis/composer-normalize": true
        }
    }
}
